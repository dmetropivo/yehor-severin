{
  "name": "yehor-severin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max_old_space_size=4096\" ./node_modules/env-cmd/bin/env-cmd.js -f env/.env.local ./node_modules/.bin/next dev",
    "build:dev": "cross-env ./node_modules/.bin/env-cmd -f env/.env.dev ./node_modules/.bin/next build",
    "build:prod": "./node_modules/.bin/env-cmd -f env/.env.prod ./node_modules/.bin/next build && npm run create-sitemap:prod",
    "start:dev": "./node_modules/.bin/env-cmd -f env/.env.dev ./node_modules/.bin/next start",
    "start:prod": "./node_modules/.bin/env-cmd -f env/.env.prod ./node_modules/.bin/next start",
    "lint": "next lint",
    "prettier": "prettier --write . --ignore-path .prettierignore",
    "create-sitemap:prod": "node_modules/.bin/env-cmd -f env/.env.prod next-sitemap"
  },
  "dependencies": {
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "axios": "^1.3.5",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "lodash": "^4.17.21",
    "next": "13.3.0",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.4",
    "next-intl": "^2.13.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^7.0.0",
    "@types/deep-equal": "^1.0.1",
    "@types/lodash": "^4.14.192",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "shortid": "^2.2.16"
  }
}
